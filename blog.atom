<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Rango Blog</title>
	<subtitle>About Rango development</subtitle>
	<link href="http://botanicus.github.com/rango/blog.atom" rel="self" />
	<link href="http://botanicus.github.com/rango" />
	{% for post in site.posts limit:1 %}
  <id>urn:uuid:{{ post.url }}</id>
	<updated>{{ post.date | date_to_xmlschema }}</updated>
	{% endfor %}
	<author>
		<name>Jakub Stastny aka Botanicus</name>
		<email>knava.bestvinensis&#x40;gmail.com</email>
	</author>
 
  {% for post in site.posts limit:5 %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="/rango{{ post.url }}" />
		<id>{{ post.url }}</id>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<summary>{{ post.perex }}</summary>
	</entry>
	{% endfor %}
</feed>
